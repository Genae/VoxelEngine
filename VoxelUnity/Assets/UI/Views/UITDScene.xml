<UITDScene>
    <UserInterface>
      <RuneOverview PlaceEnabled="true"/>
      <GameSpeed />
      <CampaignText />
      <ResourceOverview />
      <Group Spacing="15px" Alignment="Bottom" Orientation="Horizontal">
        <Button Text="START" Alpha="0.5" Click="ClickStart"/>
        <Button Text="CLEAR" Alpha="0.5" Click="ClickClear"/>
      </Group>
      <Label IsVisible="{Visible}" Text="{ErrorText}" Width="300px" Height =" 300px"></Label>
    </UserInterface>
    <EventSystem />
</UITDScene>